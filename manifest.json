{
  "manifest_version": 2,
  "name": "AnyFetch",
  "description": "Find documents related to the current page with AnyFetch.",
  "version": "1.1.2",
  "icons": {
    "16": "res/icon16.png",
    "32": "res/icon32.png",
    "48": "res/icon48.png",
    "128": "res/icon128.png"
  },
  "permissions": [
    "declarativeContent",
    "tabs",
    "storage",
    "https://*/",
    "http://*/"
  ],
  "web_accessible_resources": [
    "views/provider-callback.html",
    "views/oauth-callback.html"
  ],
  "page_action": {
    "default_icon": {
      "19": "res/icon19.png",
      "38": "res/icon38.png"
    },
    "default_popup": "views/popover.html",
    "default_title": "AnyFetch"
  },
  "background": {
    "scripts": [
      "dist/background.js"
    ],
    "persistent": false
  },
  "options_page": "views/settings.html"
}
