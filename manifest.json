{
  "manifest_version": 2,

  "name": "AnyFetch",
  "description": "Find documents related to the current page with AnyFetch.",
  "version": "0.1",

  "permissions": [
    "https://api.anyfetch.com/",
    "https://api-staging.anyfetch.com/"
  ],

  "browser_action": {
    "default_icon": {
      "19": "res/logo19.png",
      "38": "res/logo38.png"
    },
    "default_popup": "views/popover.html",
    "default_title": "AnyFetch"
  }
}
