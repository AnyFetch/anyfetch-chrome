doctype html
html
  head
    meta(charset='utf-8')
    link(rel="stylesheet", type="text/css", href="/css/style.css")
    script(type="text/javascript", src="/js/advanced-settings.js")
    title AnyFetch | Settings
  body.settings
    header.row.container
      .col-sm-10.col-sm-offset-1.col-md-8.col-md-offset-2.col-lg-6.col-lg-offset-3
        img.logo(src="/images/icons/anyfetch_icon.svg", alt="AnyFetch Icon")
        | &nbsp;Any
        b Fetch
    .vertical-align
      .row.container
        section#settings.content.col-sm-10.col-sm-offset-1.col-md-8.col-md-offset-2.col-lg-6.col-lg-offset-3.row
          h1.text-left Advanced Settings
          #settingsInputs
          hr
          .buttons.text-right
            a(href="/settings.html", title="Back") Back to settings
            | &nbsp;
            button#save.btn.btn-success Save
            | &nbsp;
            button#reset.btn.btn-warning Reset
