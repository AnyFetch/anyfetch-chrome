{
  "manifest_version": 2,
  "name": "AnyFetch",
  "description": "Find documents related to the current page with AnyFetch.",
  "version": "1.2.2",
  "icons": {
    "16": "images/icons/extension/icon16.png",
    "32": "images/icons/extension/icon32.png",
    "48": "images/icons/extension/icon48.png",
    "128": "images/icons/extension/icon128.png"
  },
  "permissions": [
    "declarativeContent",
    "tabs",
    "storage",
    "https://*/",
    "http://*/"
  ],
  "web_accessible_resources": [
    "oauth-callback.html"
  ],
  "page_action": {
    "default_icon": {
      "19": "images/icons/extension/page-action/icon19.png",
      "38": "images/icons/extension/page-action/icon38.png"
    },
    "default_popup": "popover.html",
    "default_title": "AnyFetch"
  },
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "content_security_policy": "script-src 'self' https://www.google-analytics.com; img-src *; object-src 'self'",
  "options_page": "settings.html"
}
